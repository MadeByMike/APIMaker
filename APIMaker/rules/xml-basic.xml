<?xml version="1.0" encoding="UTF-8"?>
<config table="example" resultsPerPage="20" mime="application/xml">
	<filter>
		<group>
			<field>
				<objectName>id</objectName>
				<formName>id</formName>
				<defaultValue></defaultValue>	
				<condition>equals</condition>
			</field>
			<field>
				<objectName>name</objectName>
				<formName>name</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
			<field>
				<objectName>email</objectName>
				<formName>email</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
			<field>
				<objectName>address</objectName>
				<formName>address</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
			<field>
				<objectName>lorem</objectName>
				<formName>lorem</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
			<field>
				<objectName>date</objectName>
				<formName>date</formName>
				<defaultValue></defaultValue>	
				<condition>equals</condition>
			</field>
			<field>
				<objectName>country</objectName>
				<formName>country</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
			<field>
				<objectName>latlng</objectName>
				<formName>latlng</formName>
				<defaultValue></defaultValue>	
				<condition>contains</condition>
			</field>
		</group>
	</filter>
	<sort>
		<objectName>id</objectName>
		<sortDirection>ASC</sortDirection>
	</sort>
	<errorMsg><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
		<error>No results found</error>]]>
	</errorMsg>
	<template><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
		<results>
		<summary>
		{{#summary}} 
			<page>{{page}}</page>
			<pages>{{pages}}</pages>
			<start>{{start}}</start>
			<end>{{end}}</end>
			<total>{{total}}</total>
		{{/summary}}
		</summary>
		<items>
		{{#results}}
		<item>
			<id>{{id}}</id>
			<name>{{name}}</name>
			<email>{{email}}</email>
			<address>{{address}}</address>
			<lorem>{{lorem}}</lorem>
			<date>{{date}}</date>
			<country>{{country}}</country>
			<latlng>{{latlng}}</latlng>
		</item>
		{{/results}}
		</items>
	</results>]]></template>
</config>